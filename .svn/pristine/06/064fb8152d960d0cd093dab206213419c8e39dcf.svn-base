<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_check_name"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_width="150dp"
            android:layout_marginRight="2dp"
            android:text="材料名称"/>
        <TextView
            android:id="@+id/tv_infor_code"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginRight="2dp"
            android:text="材料编码"/>

        <TextView
            android:layout_height="wrap_content"
            android:gravity="center"
            android:id="@+id/tv_check_model"
            android:layout_width="100dp"
            android:layout_marginRight="2dp"
            android:text="规格型号"/>

        <TextView
            android:layout_height="wrap_content"
            android:gravity="center"
            android:id="@+id/tv_qunantity"
            android:layout_width="70dp"
            android:text="数量"/>
        <TextView
            android:id="@+id/tv_infor_unti"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="2dp"
            android:gravity="center"
            android:text="单位"/>
        <TextView
            android:id="@+id/tv_order_date"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_width="150dp"
            android:text="盘点日期"/>

    </LinearLayout>

</LinearLayout><!-- From: file:/D:/ASproject/ZhongTieSecond/app/src/main/res/layout/adapter_check_upload.xml -->