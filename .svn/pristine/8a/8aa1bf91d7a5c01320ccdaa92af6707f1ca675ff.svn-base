package com.speedata.bean;

import com.elsw.base.db.orm.annotation.Column;
import com.elsw.base.db.orm.annotation.Table;

/** 施工队
 * Created by Administrator on 2016/3/3.
 */
@Table(name = "commonLabour")
public class CommonLabour {
    @Column(name = "LabourNM")
    // 内码
    private String LabourNM;

    @Column(name = "ProjectID")
    // 项目编号
    private String ProjectID;

    @Column(name = "LabourName")
    //施工队伍名称
    private String LabourName;

    @Column(name = "LabourLxr")
    // 联系人
    private String LabourLxr;

    @Column(name = "LabourPhone")
    // 联系电话
    private String LabourPhone;

    @Column(name = "LabourYYZZ")
    // 营业执照号
    private String LabourYYZZ;

    @Column(name = "LabourOrgCode")
    // 组织机构代码
    private String LabourOrgCode;

    @Column(name = "Remark")
    // 备注
    private String Remark;

    public String getLabourNM() {
        return LabourNM;
    }

    public void setLabourNM(String labourNM) {
        LabourNM = labourNM;
    }

    public String getProjectID() {
        return ProjectID;
    }

    public void setProjectID(String projectID) {
        ProjectID = projectID;
    }

    public String getLabourName() {
        return LabourName;
    }

    public void setLabourName(String labourName) {
        LabourName = labourName;
    }

    public String getLabourLxr() {
        return LabourLxr;
    }

    public void setLabourLxr(String labourLxr) {
        LabourLxr = labourLxr;
    }

    public String getLabourPhone() {
        return LabourPhone;
    }

    public void setLabourPhone(String labourPhone) {
        LabourPhone = labourPhone;
    }

    public String getLabourYYZZ() {
        return LabourYYZZ;
    }

    public void setLabourYYZZ(String labourYYZZ) {
        LabourYYZZ = labourYYZZ;
    }

    public String getLabourOrgCode() {
        return LabourOrgCode;
    }

    public void setLabourOrgCode(String labourOrgCode) {
        LabourOrgCode = labourOrgCode;
    }

    public String getRemark() {
        return Remark;
    }

    public void setRemark(String remark) {
        Remark = remark;
    }
}
